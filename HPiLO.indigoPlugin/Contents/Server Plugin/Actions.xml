<?xml version="1.0"?>
<Actions>
      <Action id="controlALARM">
        <Name>Control Alarm</Name>
        <CallbackMethod>controlAlarm</CallbackMethod>
        <ConfigUI>
           	<Field id="partition" type="menu" defaultValue="1">
				<Label>Partition Number:</Label>
				<List>
					<Option value="1">1</Option>
					<Option value="2">2</Option>
					<Option value="3">3</Option>
					<Option value="4">4</Option>
				</List>
            </Field>
            <Field id="action" type="menu" defaultValue="1">
				<Label>Action:</Label>
				<List>
					<Option value="ARM">Arm</Option>
					<Option value="SLEEP">Sleep</Option>
                    <Option value="STAY">Stay</Option>
					<Option value="DISARM">Disarm </Option>

				</List>
            </Field>
        </ConfigUI>
    </Action>

    <Action id="clearPoweronTime" deviceFilter="self.MainILO3">
        <Name>Clear Server Power On Time</Name>
        <CallbackMethod>action_Simple</CallbackMethod>
    </Action>

	    <Action id="PowerOn" deviceFilter="self.MainILO3">
        <Name>Turn Server Power On</Name>
        <CallbackMethod>action_Simple</CallbackMethod>
    </Action>
		    <Action id="PowerOff" deviceFilter="self.MainILO3">
        <Name>Turn Server Power On</Name>
        <CallbackMethod>action_Simple</CallbackMethod>
    </Action>
			    <Action id="activateProLicense" deviceFilter="self.MainILO3">
        <Name>Install HP iLO Advanced License Key iLO3</Name>
        <CallbackMethod>action_Simple</CallbackMethod>
				<ConfigUI>
					<Field id="licensekey" type="menu" defaultValue="34RQQ-6D5R4-G8NW3-LBMG6-MLJXR">
				<Label>Key:</Label>
				<List>
					<Option value="34RQQ-6D5R4-G8NW3-LBMG6-MLJXR">34RQQ-6D5R4-G8NW3-LBMG6-MLJXR</Option>
					<Option value="325WC-J9QJ7-495NG-CP7WZ-7GJMM">325WC-J9QJ7-495NG-CP7WZ-7GJMM</Option>
				</List>
            </Field>
				</ConfigUI>
    </Action>



  
</Actions>

